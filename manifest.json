{
  "name": "Dev.to Dark Theme",
  "version": "0.1",
  "description": "This is dark theme for dev.to website",
  "permissions": ["activeTab"],
  "background": {
    "scripts": ["jquery/jquery.js"],
    "persistent": false
  },
  "icons": {
    "16": "images/devtodark16.png",
    "32": "images/devtodark32.png",
    "48": "images/devtodark48.png",
    "128": "images/devtodark128.png"
  },
  "content_scripts": [{
    "css": ["style.css"],
    "matches": ["https://dev.to/*"],
    "js":["update.js"]
  }],
  "manifest_version": 2
}
